;\\\\n  date: Date;\\\\n}\\\\n\\\\nexport default function App() {\\\\n  const [currentTime, setCurrentTime] = useState(new Date());\\\\n\\\\n  // Mock prayer times (in a real app, these would be calculated based on location)\\\\n  const today = new Date();\\\\n  const prayerTimes: PrayerTime[] = [\\\\n    {\\\\n      name: 'Fajr',\\\\n      time: '05:30 AM',\\\\n      date: new Date(today.getFullYear(), today.getMonth(), today.getDate(), 5, 30, 0),\\\\n    },\\\\n    {\\\\n      name: 'Dhuhr',\\\\n      time: '12:45 PM',\\\\n      date: new Date(today.getFullYear(), today.getMonth(), today.getDate(), 12, 45, 0),\\\\n    },\\\\n    {\\\\n      name: 'Asr',\\\\n      time: '03:30 PM',\\\\n      date: new Date(today.getFullYear(), today.getMonth(), today.getDate(), 15, 30, 0),\\\\n    },\\\\n    {\\\\n      name: 'Maghrib',\\\\n      time: '06:15 PM',\\\\n      date: new Date(today.getFullYear(), today.getMonth(), today.getDate(), 18, 15, 0),\\\\n    },\\\\n    {\\\\n      name: 'Isha',\\\\n      time: '07:45 PM',\\\\n      date: new Date(today.getFullYear(), today.getMonth(), today.getDate(), 19, 45, 0),\\\\n    },\\\\n  ];\\\\n\\\\n  useEffect(() => {\\\\n    const timer = setInterval(() => {\\\\n      setCurrentTime(new Date());\\\\n    }, 1000);\\\\n\\\\n    return () => clearInterval(timer);\\\\n  }, []);\\\\n\\\\n  // Find next prayer\\\\n  const now = currentTime.getTime();\\\\n  const nextPrayerIndex = prayerTimes.findIndex((prayer) => prayer.date.getTime() > now);\\\\n  const nextPrayer =\\\\n    nextPrayerIndex !== -1 ? prayerTimes[nextPrayerIndex] : prayerTimes[0];\\\\n\\\\n  // Hijri date (mock - in real app would use Islamic calendar API)\\\\n  const hijriDate = '17 Sha\\\\\\\\'ban 1447';\\\\n  const gregorianDate = currentTime.toLocaleDateString('en-US', {\\\\n    weekday: 'long',\\\\n    year: 'numeric',\\\\n    month: 'long',\\\\n    day: 'numeric',\\\\n  });\\\\n\\\\n  return (\\\\n    <div className=\\\\\\\"size-full min-h-screen bg-gradient-to-br from-slate-900 via-slate-800 to-slate-900 text-white overflow-auto\\\\\\\">\\\\n      {/* Background Image Overlay */}\\\\n      <div className=\\\\\\\"absolute inset-0 opacity-10\\\\\\\">\\\\n        <ImageWithFallback\\\\n          src=\\\\\\\"https://images.unsplash.com/photo-1646897343393-bf541e582215?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxpc2xhbWljJTIwbW9zcXVlJTIwYXJjaGl0ZWN0dXJlJTIwYmx1ZXxlbnwxfHx8fDE3NzIxNDI0MjB8MA&ixlib=rb-4.1.0&q=80&w=1080&utm_source=figma&utm_medium=referral\\\\\\\"\\\\n          alt=\\\\\\\"Islamic pattern\\\\\\\"\\\\n          className=\\\\\\\"size-full object-cover\\\\\\\"\\\\n        />\\\\n      </div>\\\\n\\\\n      <div className=\\\\\\\"relative z-10 p-8 max-w-7xl mx-auto\\\\\\\">\\\\n        {/* Header */}\\\\n        <header className=\\\\\\\"flex items-center justify-between mb-12\\\\\\\">\\\\n          <div>\\\\n            <h1 className=\\\\\\\"text-5xl font-bold mb-2\\\\\\\">Azzan Prayer</h1>\\\\n            <p className=\\\\\\\"text-slate-400 text-lg\\\\\\\">Prayer times for your daily worship</p>\\\\n          </div>\\\\n          <div className=\\\\\\\"flex items-center gap-6 text-right\\\\\\\">\\\\n            <div className=\\\\\\\"flex flex-col items-end gap-2\\\\\\\">\\\\n              <div className=\\\\\\\"flex items-center gap-2 text-emerald-400\\\\\\\">\\\\n                <MapPin className=\\\\\\\"size-5\\\\\\\" />\\\\n                <span className=\\\\\\\"text-lg\\\\\\\">New York, USA</span>\\\\n              </div>\\\\n              <div className=\\\\\\\"flex items-center gap-2 text-slate-300\\\\\\\">\\\\n                <Compass className=\\\\\\\"size-5\\\\\\\" />\\\\n                <span>Qibla: 58Â° NE</span>\\\\n              </div>\\\\n            </div>\\\\n          </div>\\\\n        </header>\\\\n\\\\n        {/* Current Time and Date */}\\\\n        <div className=\\\\\\\"bg-slate-800/40 backdrop-blur-sm rounded-3xl p-8 mb-8 border border-slate-700/50\\\\\\\">\\\\n          <div className=\\\\\\\"flex items-center justify-between\\\\\\\">\\\\n            <div className=\\\\\\\"flex items-center gap-4\\\\\\\">\\\\n              <Clock className=\\\\\\\"size-8 text-emerald-400\\\\\\\" />\\\\n              <div>\\\\n                <p className=\\\\\\\"text-sm text-slate-400\\\\\\\">Current Time</p>\\\\n                <p className=\\\\\\\"text-4xl font-mono font-bold\\\\\\\">\\\\n                  {currentTime.toLocaleTimeString('en-US', {\\\\n                    hour: '2-digit',\\\\n                    minute: '2-digit',\\\\n                    second: '2-digit',\\\\n                  })}\\\\n                </p>\\\\n              </div>\\\\n            </div>\\\\n            <div className=\\\\\\\"flex items-center gap-4\\\\\\\">\\\\n              <Calendar className=\\\\\\\"size-8 text-emerald-400\\\\\\\" />\\\\n              <div className=\\\\\\\"text-right\\\\\\\">\\\\n                <p className=\\\\\\\"text-sm text-slate-400\\\\\\\">Date</p>\\\\n                <p className=\\\\\\\"text-xl font-semibold\\\\\\\">{gregorianDate}</p>\\\\n                <p className=\\\\\\\"text-base text-emerald-400\\\\\\\">{hijriDate}</p>\\\\n              </div>\\\\n            </div>\\\\n          </div>\\\\n        </div>\\\\n\\\\n        {/* Countdown to Next Prayer */}\\\\n        <div className=\\\\\\\"bg-gradient-to-br from-slate-800/60 to-slate-800/40 backdrop-blur-sm rounded-3xl p-10 mb-8 border border-emerald-500/20 shadow-xl\\\\\\\">\\\\n          <CountdownTimer targetTime={nextPrayer.date} prayerName={nextPrayer.name} />\\\\n        </div>\\\\n\\\\n        {/* Prayer Times Grid */}\\\\n        <div className=\\\\\\\"grid gap-4\\\\\\\">\\\\n          <h2 className=\\\\\\\"text-3xl font-semibold mb-2\\\\\\\">Today's Prayer Times</h2>\\\\n          {prayerTimes.map((prayer, index) => {\\\\n            const isNext = prayer.name === nextPrayer.name;\\\\n            const isPassed = prayer.date.getTime() < now;\\\\n\\\\n            return (\\\\n              <PrayerTimeCard\\\\n                key={prayer.name}\\\\n                name={prayer.name}\\\\n                time={prayer.time}\\\\n                isNext={isNext}\\\\n                isPassed={isPassed}\\\\n              />\\\\n            );\\\\n          })}\\\\n        </div>\\\\n\\\\n        {/* Footer Note */}\\\\n        <div className=\\\\\\\"mt-12 text-center text-slate-400 text-sm\\\\\\\">\\\\n          <p>Prayer times are calculated based on your location</p>\\\\n          <p className=\\\\\\\"mt-2\\\\\\\">May Allah accept your prayers ðŸ¤²</p>\\\\n        </div>\\\\n      </div>\\\\n    </div>\\\\n  );\\\\n}\\\\n\\\"}\"}"},{"id":"556da152-7494-4c81-a9d3-007e4a08827d","index":3,"partType":"code-chat-legacy-s3-code-snapshot","partTypeVersion":1,"contentJson":"{\"codeSnapshotKey\":\"dd39b11ddb5bf0737231ec8860738b77a57ca0d511c3f3c4ea786531b3ddcbed\"}"}],"createdAt":"2026-02-26T21:47:38Z","updatedAt":"2026-02-26T21:47:38Z","nodeGuid":"1:14","userId":"1530095214077209014","scenegraphSentAt":"1772142433379"},{"id":"e7f95eca-9faf-40cc-ad15-cd6d0949e7a3","index":9,"role":"tool","startFileSeqNum":"1","endFileSeqNum":"47","parts":[{"id":"71e84c67-f51c-4768-9eb2-fd94eefcd176","index":0,"partType":"tool-result-json-DO-NOT-USE-IN-PROD","partTypeVersion":1,"contentJson":"{\"toolCallId\":\"tooluse_lOYyTTmJUr0O1k4G6Nd3Vd\",\"toolName\":\"write_tool\",\"resultJson\":\"{\\\"success\\\":true,\\\"message\\\":\\\"Successfully rewrote file /src/app/App.tsx\\\"}\"}"},{"id":"4a35a886-1ef6-4a24-8e0d-e4892da43bae","index":1,"partType":"code-chat-model-config-version","partTypeVersion":1,"contentJson":"{\"modelConfigVersion\":\"claude-4-5-sonnet_0.0.0\"}"}],"createdAt":"2026-02-26T21:47:38Z","updatedAt":"2026-02-26T21:47:38Z","nodeGuid":"1:15","userId":"1530095214077209014","scenegraphSentAt":"1772142451769"},{"id":"bd231274-ea02-44da-939e-fad08f7af721","index":10,"role":"assistant","startFileSeqNum":"1","endFileSeqNum":"47","parts":[{"id":"4c7cb