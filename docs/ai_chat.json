\\\n      </div>\\\\n      <span className={`text-3xl font-mono ${isNext ? 'font-bold' : ''}`}>{time}</span>\\\\n    </div>\\\\n  );\\\\n}\\\\n\\\"}\"}"},{"id":"b87b64d1-4e5b-4f9f-89a8-639e70910644","index":4,"partType":"tool-call-json-DO-NOT-USE-IN-PROD","partTypeVersion":1,"contentJson":"{\"toolCallId\":\"tooluse_cnXbz5fxBrvjIAndgnKIw9\",\"toolName\":\"write_tool\",\"argsJson\":\"{\\\"path\\\":\\\"/src/app/components/CountdownTimer.tsx\\\",\\\"file_text\\\":\\\"import { useEffect, useState } from 'react';\\\\n\\\\ninterface CountdownTimerProps {\\\\n  targetTime: Date;\\\\n  prayerName: string;\\\\n}\\\\n\\\\nexport function CountdownTimer({ targetTime, prayerName }: CountdownTimerProps) {\\\\n  const [timeLeft, setTimeLeft] = useState(calculateTimeLeft());\\\\n\\\\n  function calculateTimeLeft() {\\\\n    const now = new Date();\\\\n    const diff = targetTime.getTime() - now.getTime();\\\\n\\\\n    if (diff <= 0) {\\\\n      return { hours: 0, minutes: 0, seconds: 0 };\\\\n    }\\\\n\\\\n    const hours = Math.floor(diff / (1000 * 60 * 60));\\\\n    const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));\\\\n    const seconds = Math.floor((diff % (1000 * 60)) / 1000);\\\\n\\\\n    return { hours, minutes, seconds };\\\\n  }\\\\n\\\\n  useEffect(() => {\\\\n    const timer = setInterval(() => {\\\\n      setTimeLeft(calculateTimeLeft());\\\\n    }, 1000);\\\\n\\\\n    return () => clearInterval(timer);\\\\n  }, [targetTime]);\\\\n\\\\n  return (\\\\n    <div className=\\\\\\\"text-center space-y-4\\\\\\\">\\\\n      <p className=\\\\\\\"text-lg text-slate-300\\\\\\\">Time until {prayerName}</p>\\\\n      <div className=\\\\\\\"flex justify-center gap-6\\\\\\\">\\\\n        <TimeUnit value={timeLeft.hours} label=\\\\\\\"Hours\\\\\\\" />\\\\n        <div className=\\\\\\\"text-5xl font-bold text-white/50\\\\\\\">:</div>\\\\n        <TimeUnit value={timeLeft.minutes} label=\\\\\\\"Minutes\\\\\\\" />\\\\n        <div className=\\\\\\\"text-5xl font-bold text-white/50\\\\\\\">:</div>\\\\n        <TimeUnit value={timeLeft.seconds} label=\\\\\\\"Seconds\\\\\\\" />\\\\n      </div>\\\\n    </div>\\\\n  );\\\\n}\\\\n\\\\nfunction TimeUnit({ value, label }: { value: number; label: string }) {\\\\n  return (\\\\n    <div className=\\\\\\\"flex flex-col items-center\\\\\\\">\\\\n      <div className=\\\\\\\"bg-slate-700/50 rounded-xl px-6 py-4 min-w-24\\\\\\\">\\\\n        <span className=\\\\\\\"text-5xl font-bold font-mono\\\\\\\">\\\\n          {value.toString().padStart(2, '0')}\\\\n        </span>\\\\n      </div>\\\\n      <span className=\\\\\\\"text-sm text-slate-400 mt-2\\\\\\\">{label}</span>\\\\n    </div>\\\\n  );\\\\n}\\\\n\\\"}\"}"},{"id":"1e9e190d-8105-4472-a5d2-8365adbf74b2","index":5,"partType":"code-chat-legacy-s3-code-snapshot","partTypeVersion":1,"contentJson":"{\"codeSnapshotKey\":\"dd39b11ddb5bf0737231ec8860738b77a57ca0d511c3f3c4ea786531b3ddcbed\"}"}],"createdAt":"2026-02-26T21:47:38Z","updatedAt":"2026-02-26T21:47:38Z","nodeGuid":"1:10","userId":"1530095214077209014","scenegraphSentAt":"1772142420775"},{"id":"3efc7860-573b-4228-8160-f926d0cbfc22","index":7,"role":"tool","startFileSeqNum":"1","endFileSeqNum":"47","parts":[{"id":"c2bc092d-f03c-41ed-b262-a82a211fe664","index":0,"partType":"tool-result-json-DO-NOT-USE-IN-PROD","partTypeVersion":1,"contentJson":"{\"toolCallId\":\"tooluse_VVJOjEnQRSlLGKsDfIR87l\",\"toolName\":\"unsplash_tool\",\"resultJson\":\"{\\\"success\\\":true,\\\"imageUrl\\\":\\\"https://images.unsplash.com/photo-1646897343393-bf541e582215?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxpc2xhbWljJTIwbW9zcXVlJTIwYXJjaGl0ZWN0dXJlJTIwYmx1ZXxlbnwxfHx8fDE3NzIxNDI0MjB8MA&ixlib=rb-4.1.0&q=80&w=1080&utm_source=figma&utm_medium=referral\\\"}\"}"},{"id":"c34bc445-c464-4bc4-9dba-5098d9988a2a","index":1,"partType":"tool-result-json-DO-NOT-USE-IN-PROD","partTypeVersion":1,"contentJson":"{\"toolCallId\":\"tooluse_wGtRvo7dPUroEmLQptw9bB\",\"toolName\":\"write_tool\",\"re